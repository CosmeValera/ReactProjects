#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/covr/OneDrive - gmv.com/Desktop/Cosme (Local)/ReactProjects/monorepos/monorepo-beginners/node_modules/.pnpm/@monorepo-beginners+module-b@1.1.0/node_modules/@monorepo-beginners/module-b/node_modules:/mnt/c/Users/covr/OneDrive - gmv.com/Desktop/Cosme (Local)/ReactProjects/monorepos/monorepo-beginners/node_modules/.pnpm/@monorepo-beginners+module-b@1.1.0/node_modules/@monorepo-beginners/node_modules:/mnt/c/Users/covr/OneDrive - gmv.com/Desktop/Cosme (Local)/ReactProjects/monorepos/monorepo-beginners/node_modules/.pnpm/@monorepo-beginners+module-b@1.1.0/node_modules:/mnt/c/Users/covr/OneDrive - gmv.com/Desktop/Cosme (Local)/ReactProjects/monorepos/monorepo-beginners/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/covr/OneDrive - gmv.com/Desktop/Cosme (Local)/ReactProjects/monorepos/monorepo-beginners/node_modules/.pnpm/@monorepo-beginners+module-b@1.1.0/node_modules/@monorepo-beginners/module-b/node_modules:/mnt/c/Users/covr/OneDrive - gmv.com/Desktop/Cosme (Local)/ReactProjects/monorepos/monorepo-beginners/node_modules/.pnpm/@monorepo-beginners+module-b@1.1.0/node_modules/@monorepo-beginners/node_modules:/mnt/c/Users/covr/OneDrive - gmv.com/Desktop/Cosme (Local)/ReactProjects/monorepos/monorepo-beginners/node_modules/.pnpm/@monorepo-beginners+module-b@1.1.0/node_modules:/mnt/c/Users/covr/OneDrive - gmv.com/Desktop/Cosme (Local)/ReactProjects/monorepos/monorepo-beginners/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../index.js" "$@"
else
  exec node  "$basedir/../../index.js" "$@"
fi
